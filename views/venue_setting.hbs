  <body class="nav-md">
    <div class="container body">
      <div class="main_container setting-page">
        <div class="col-md-3 left_col">
          <div class="left_col scroll-view">
            <div class="navbar nav_title" style="border: 0;" text-align: center;>
              <a href="/" class="site_title"><img src="/images/logo-top-left.png" width="180px"/></a>
            </div>

            <div class="clearfix"></div>

            <br />

            <!-- sidebar menu -->
            <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
              <div class="menu_section">
                <ul class="nav side-menu">

                </ul>
              </div>

            </div>
            <!-- /sidebar menu -->

            <!-- /menu footer buttons -->
            <div class="sidebar-footer hidden-small">
              <a data-toggle="tooltip" data-placement="top" title="Settings">
                <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
              </a>
              <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
              </a>
              <a data-toggle="tooltip" data-placement="top" title="Lock">
                <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
              </a>
              <a data-toggle="tooltip" data-placement="top" title="Logout" href="login">
                <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
              </a>
            </div>
            <!-- /menu footer buttons -->
          </div>
        </div>

        <!-- top navigation -->
        {{> headers}}

        <!-- page content -->
        <div class="hidden-div">
            <input type="hidden" id="venue_id" value="{{venue.venue_id}}">
            <input type="hidden" id="venue_notify_option" value="{{notify_option}}">
        </div>
        <div class="right_col" role="main">
            <div class="" style="margin-left: 50px;">
                <div class="page-title">
                  <div class="title_left">
                    <h3 style="color:#71706e; font-size: 28px; margin-top: 30px;">Venue Settings</h3>
                  </div>
                </div>

                <div class="clearfix"></div>

                <div class="row">
                  <div class="col-md-12">
                    <div class="x_panel grey_back">
                      <div class="x_content">
                        <div class="control_row" style="padding-left: 60px;">
                            <span style="font-size: 30px; font-weight: 300; width: 250px; display: inline-block; color:#71706e;"> Brightness </span>
                            <span style="width: calc(100% - 450px); display: inline-block; vertical-align: text-bottom;">
                                <input id="brightness" data-slider-id='brightnessSlider' type="text" data-slider-min="0" data-slider-max="100" data-slider-step="1" data-slider-value="1"/>
                            </span>
                            <span id="brightness_val" style="width:100px;text-align: center; color:grey; display: inline-block; font-size: 25px;">
                            1%
                            </span>
                        </div>
                        <div class="control_row" style="padding-left: 60px;">
                            <div style="font-size: 30px; font-weight: 300; width: 250px; display: inline-block; vertical-align: top; color:#71706e; margin-top: 35px;">
                                LED Colors
                            </div>
                            <div style="width: calc(100% - 260px); display: inline-block;">
                                <span style="font-size: 25px; width:33.33%; color:#c2c2c2">
                                    <svg height="55" width="52" style="">
                                        <rect x="0" cy="0" width="50" height="50" rx="20" ry="20" stroke="black" stroke-width="0" fill="#2cf80c" id="vacant_col" />
                                    </svg>
                                    <span style="vertical-align: top; line-height: 55px;">available</span>
                                </span><span style="font-size: 25px; width:33.33%; color:#c2c2c2">
                                    <svg height="55" width="52" style="">
                                        <rect x="0" cy="0" width="50" height="50" rx="20" ry="20" stroke="black" stroke-width="0" fill="red" id="occupied_col" />
                                    </svg>
                                    <span style="vertical-align: top; line-height: 55px;">occupied</span>
                                </span><span style="font-size: 18px; width:33.33%; color:#c2c2c2">
                                    <svg height="55" width="52" style="">
                                        <rect x="0" cy="0" width="50" height="50" rx="20" ry="20" stroke="black" stroke-width="0" fill="blue" id="vacant_handicap" />
                                    </svg>
                                    <span style="vertical-align: top; line-height: 25px;">available<br/>(handicap accessible)</span>
                                </span>
                                <div style="text-align: center;">
                                    <span style="background-color: #ababab; padding: 3px 20px; border-radius: 20px; font-size: 20px; color: #fff; cursor: pointer;">
                                        reset colors
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="control_row" style="position:relative; height: 180px; padding-left: 60px; padding-top: 27px;" id="battery_low_row">
