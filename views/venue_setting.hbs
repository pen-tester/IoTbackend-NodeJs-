                                <span style="width: 100%; display: inline-block; vertical-align: text-bottom;">
                                    <input id="acute_maintenancethreshold" data-slider-id='acute_maintenance_thresholdSlider' type="text" data-slider-min="0" data-slider-max="100" data-slider-step="1" data-slider-value="1"/>
