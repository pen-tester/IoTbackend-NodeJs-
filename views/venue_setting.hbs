                                <input type="checkbox" checked="checked" id="low_battery_enabled" class="big"> <label for="low_battery_enabled">Low Battery</label>
                            </span>
                            <div style="margin-left: 60%; margin-top: 15px; height: 30px;">
                                <span style="width: 100%; display: inline-block; vertical-align: text-bottom;">
                                    <input id="battery_low_threshold" data-slider-id='battery_low_thresholdSlider' type="text" data-slider-min="0" data-slider-max="100" data-slider-step="1" data-slider-value="1"/>
                                </span>
                            </div>
                            <div style="color:#9a9a9a;">
                                <div style="font-size: 25px;margin-left: 28px;">
                                    Notify me when battery reaches <span id="battery_low_threshold_val">0%</span>
                                </div>
                                <div style="text-align:center; font-size: 25px;">
                                    <span>
                                        <input type="checkbox" class="viaDashboard" id="viadashboard1" checked="checked">
                                        <label for="viadashboard1">
                                            <span style="vertical-align: middle; margin-right: 20px;">via dashboard</span>
                                        </label>
                                    </span>
                                    <span>
                                        <input type="checkbox" class="viaEmail" id="viaemail1" checked="checked">
                                        <label for="viaemail1">
                                            via email
                                        </label>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
            </div>

